{
  "version": "2.0.0",
  "orchestration": {
    "mode": "hybrid",
    "parallel_execution": true,
    "max_agents": 20,
    "default_agent_type": "claude",
    "agent_startup_delay": 0.5
  },
  
  "communication": {
    "protocol": "enhanced",
    "relay_enabled": true,
    "message_format": "json",
    "broadcast_channel": ".orchestrator/messages",
    "inter_agent_delay": 0.2,
    "max_retries": 3,
    "timeout": 30,
    "mcp_enabled": true,
    "mcp_servers": [
      "filesystem",
      "git",
      "docker",
      "kubernetes",
      "postgres",
      "redis"
    ]
  },
  
  "agents": {
    "claude": {
      "executable": "claude",
      "flags": ["--continue"],
      "sub_agents_enabled": true,
      "thinking_modes": {
        "standard": "",
        "think": "think",
        "think_hard": "think hard",
        "think_harder": "think harder",
        "ultrathink": "ultrathink"
      },
      "hooks": {
        "pre_task": ".taco/hooks/pre-task.sh",
        "post_task": ".taco/hooks/post-task.sh",
        "on_error": ".taco/hooks/on-error.sh"
      },
      "headless_mode": false,
      "cache_prompts": true,
      "cache_duration": 3600
    },
    
    "openai": {
      "executable": "openai-cli",
      "api_key_env": "OPENAI_API_KEY",
      "model": "gpt-4-turbo",
      "temperature": 0.7,
      "max_tokens": 4096
    },
    
    "anthropic_api": {
      "api_key_env": "ANTHROPIC_API_KEY",
      "model": "claude-3-opus-20240229",
      "max_tokens": 4096,
      "temperature": 0.7
    },
    
    "gemini": {
      "executable": "gemini",
      "flags": ["--yolo"],
      "api_key_env": "GEMINI_API_KEY",
      "model": "gemini-ultra"
    },
    
    "codex": {
      "executable": "codex",
      "flags": ["--full-auto"],
      "api_key_env": "CODEX_API_KEY"
    },
    
    "llama": {
      "executable": "ollama",
      "model": "llama3:70b",
      "local": true,
      "gpu_enabled": true
    },
    
    "mistral": {
      "executable": "mistral-cli",
      "api_key_env": "MISTRAL_API_KEY",
      "model": "mistral-large"
    },
    
    "custom": {
      "executable": "custom-agent",
      "config_file": ".taco/custom-agent.json"
    }
  },
  
  "sub_agents": {
    "auto_create": true,
    "templates": {
      "code-reviewer": {
        "description": "Expert code review specialist",
        "tools": ["Read", "Grep", "Bash"],
        "proactive": true
      },
      "test-runner": {
        "description": "Comprehensive testing specialist",
        "tools": ["Read", "Write", "Edit", "Bash"],
        "proactive": true
      },
      "debugger": {
        "description": "Debugging and error resolution expert",
        "tools": ["Read", "Edit", "Bash", "Grep"],
        "proactive": false
      },
      "architect": {
        "description": "System design and architecture expert",
        "tools": ["Read", "Write", "WebFetch"],
        "proactive": false
      },
      "data-scientist": {
        "description": "Data analysis and ML specialist",
        "tools": ["Read", "Write", "Bash", "WebFetch"],
        "proactive": false
      },
      "devops": {
        "description": "Infrastructure and deployment specialist",
        "tools": ["Read", "Write", "Edit", "Bash"],
        "proactive": true
      }
    }
  },
  
  "memory": {
    "type": "sqlite",
    "path": ".taco/memory.db",
    "context_window": 200000,
    "semantic_search": true,
    "vector_db": "chroma",
    "embeddings_model": "text-embedding-ada-002"
  },
  
  "monitoring": {
    "enabled": true,
    "dashboard_window": 1,
    "test_window": 2,
    "update_interval": 1,
    "metrics": {
      "track_tokens": true,
      "track_costs": true,
      "track_errors": true,
      "track_performance": true
    },
    "logging": {
      "level": "INFO",
      "file": ".orchestrator/orchestrator.log",
      "rotate": true,
      "max_size": "100MB"
    }
  },
  
  "testing": {
    "mandatory": true,
    "min_coverage": 80,
    "frameworks": {
      "javascript": ["jest", "mocha", "vitest"],
      "python": ["pytest", "unittest"],
      "go": ["go test"],
      "rust": ["cargo test"]
    },
    "parallel_execution": true,
    "fail_fast": false,
    "auto_fix": true
  },
  
  "deployment": {
    "environments": ["local", "docker", "kubernetes", "cloud"],
    "auto_detect": true,
    "port_range": {
      "start": 3000,
      "end": 9999
    },
    "service_discovery": true,
    "health_checks": true
  },
  
  "performance": {
    "parallel_agents_limit": 20,
    "memory_limit": "4GB",
    "cpu_cores": "auto",
    "gpu_enabled": false,
    "batch_operations": true,
    "cache_enabled": true
  },
  
  "integrations": {
    "github": {
      "enabled": true,
      "pr_reviews": true,
      "auto_commit": false,
      "branch_protection": true
    },
    "gitlab": {
      "enabled": false,
      "api_url": ""
    },
    "jira": {
      "enabled": false,
      "api_url": "",
      "project_key": ""
    },
    "slack": {
      "enabled": false,
      "webhook_url": ""
    },
    "discord": {
      "enabled": false,
      "webhook_url": ""
    }
  },
  
  "advanced": {
    "swarm_intelligence": true,
    "self_organizing": true,
    "fault_tolerance": true,
    "auto_scaling": true,
    "load_balancing": true,
    "circuit_breaker": true,
    "retry_strategy": "exponential_backoff",
    "consensus_protocol": "raft",
    "distributed_tracing": true
  }
}